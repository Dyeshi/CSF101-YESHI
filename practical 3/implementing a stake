class stack:
  
  def __init__(self):
    self.item = []

  def is_empty(self):
     return len(self.item) == 0

  def push(self, item):
    self.item.append(item)

  def pop(self):
    if not self.is_empty():
        return self.item.pop()
    else:
        raise IndexError("stake is  empty")

  def peek(self):
    if not self.is_empty():
        return self.item[-1]
    else:
        raise IndexError("stack is empty")

  def size(self):
    return len(self.item)

stack = stack()
stack.push(1)
stack.push(2)
stack.push(3)
print(stack.pop())
print(stack.peek())
print(stack.size())